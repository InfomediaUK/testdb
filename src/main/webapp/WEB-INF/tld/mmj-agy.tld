<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE taglib
  PUBLIC "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.2//EN"
  "http://java.sun.com/dtd/web-jsptaglibrary_1_2.dtd">
<taglib>
  <tlib-version>1.2</tlib-version>
  <jsp-version>1.2</jsp-version>
  <short-name>mmj</short-name>
  <display-name>MMJ</display-name>
  <description>MMJ library</description>

  <tag>
    <name>consultant</name>
    <tag-class>com.helmet.application.agy.taglib.ConsultantTag</tag-class>
    <body-content>JSP</body-content>
    <description>
    	Sets the current consultant in a 'scope'
    </description>
    <attribute>
        <name>var</name>
        <required>true</required>
        <rtexprvalue>false</rtexprvalue>
    </attribute>
    <attribute>
        <name>scope</name>
        <required>false</required>
        <rtexprvalue>false</rtexprvalue>
    </attribute>
  </tag>

  <tag>
    <name>agency</name>
    <tag-class>com.helmet.application.agy.taglib.AgencyTag</tag-class>
    <body-content>JSP</body-content>
    <description>
    	Sets the current agency in a 'scope'
    </description>
    <attribute>
        <name>var</name>
        <required>true</required>
        <rtexprvalue>false</rtexprvalue>
    </attribute>
    <attribute>
        <name>scope</name>
        <required>false</required>
        <rtexprvalue>false</rtexprvalue>
    </attribute>
  </tag>

  <tag>
    <name>hasAccess</name>
    <tag-class>com.helmet.application.agy.taglib.HasAccessTag</tag-class>
    <body-content>JSP</body-content>
    <description>
    	Checks if the currently logged in consultant has access to the forward
    </description>
    <attribute>
        <name>forward</name>
        <required>true</required>
        <rtexprvalue>false</rtexprvalue>
    </attribute>
  </tag>

  <tag>
    <name>hasNoAccess</name>
    <tag-class>com.helmet.application.agy.taglib.HasNoAccessTag</tag-class>
    <body-content>JSP</body-content>
    <description>
    	Checks if the currently logged in consultant has NO access to the forward
    </description>
    <attribute>
        <name>forward</name>
        <required>true</required>
        <rtexprvalue>false</rtexprvalue>
    </attribute>
  </tag>

  <tag>
    <name>bookingStatusCount</name>
    <tag-class>com.helmet.application.agy.taglib.BookingStatusCountTag</tag-class>
    <body-content>JSP</body-content>
    <description>
    	Returns the count of bookings for the requested status - this checks for bookingStatusCounts in the request
    </description>
    <attribute>
        <name>status</name>
        <required>true</required>
        <rtexprvalue>false</rtexprvalue>
    </attribute>
  </tag>

  <tag>
    <name>shiftStatusCount</name>
    <tag-class>com.helmet.application.agy.taglib.ShiftStatusCountTag</tag-class>
    <body-content>JSP</body-content>
    <description>
    	Returns the count of shifts for the requested status - this checks for shiftStatusCounts in the request
    </description>
    <attribute>
        <name>status</name>
        <required>true</required>
        <rtexprvalue>false</rtexprvalue>
    </attribute>
  </tag>

  <tag>
    <name>shiftsToActivateCount</name>
    <tag-class>com.helmet.application.agy.taglib.ShiftsToActivateCountTag</tag-class>
    <body-content>JSP</body-content>
    <description>
    	Returns the count of shifts to activate - this checks for shiftsToActivateCount in the request
    </description>
  </tag>

  <tag>
    <name>shiftsOutstandingCount</name>
    <tag-class>com.helmet.application.agy.taglib.ShiftsOutstandingCountTag</tag-class>
    <body-content>JSP</body-content>
    <description>
    	Returns the count of shifts outstanding (activated by not entered) - this checks for shiftsOutstandingCount in the request
    </description>
  </tag>

  <tag>
    <name>bookingGradeApplicantDateStatusCount</name>
    <tag-class>com.helmet.application.agy.taglib.BookingGradeApplicantDateStatusCountTag</tag-class>
    <body-content>JSP</body-content>
    <description>
    	Returns the count of bookings for the requested status - this checks for bookingGradeApplicantDateStatusCounts in the request
    </description>
    <attribute>
        <name>status</name>
        <required>true</required>
        <rtexprvalue>false</rtexprvalue>
    </attribute>
  </tag>

  <tag>
    <name>shiftWorkedStatusCount</name>
    <tag-class>com.helmet.application.agy.taglib.ShiftWorkedStatusCountTag</tag-class>
    <body-content>JSP</body-content>
    <description>
    	Returns the count of shifts for the requested worked status - this checks for shiftWorkedStatusCounts in the request
    </description>
    <attribute>
        <name>status</name>
        <required>true</required>
        <rtexprvalue>false</rtexprvalue>
    </attribute>
  </tag>

  <tag>
    <name>agencyInvoiceStatusCount</name>
    <tag-class>com.helmet.application.agy.taglib.AgencyInvoiceStatusCountTag</tag-class>
    <body-content>JSP</body-content>
    <description>
    	Returns the count of agencyInvoices for the requested status - this checks for agencyInvoiceStatusCounts in the request
    </description>
    <attribute>
        <name>status</name>
        <required>true</required>
        <rtexprvalue>false</rtexprvalue>
    </attribute>
  </tag>

</taglib>
